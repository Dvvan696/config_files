# Ubuntu stuff
alias update='pkg update'

#Git stuff
alias githistory="git log --oneline --graph --decorate --all"

# Start Ubuntu
alias ubuntu='~/start-ubuntu.sh'

function gitupload() {
	
	if [ -n "$1" ]; then
		git add .
		git commit -m "$1"
		git push
	else
		echo "Provide a commit message"
	fi
		
}


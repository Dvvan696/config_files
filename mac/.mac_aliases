              brew upgrade &&
              brew cask upgrade"

alias pycoding="cd ~/MegaSync/Coding/Python/ &&
                open ."

alias ccoding="cd ~/MegaSync/Coding/C && 
	      open ."

alias githistory="git log --oneline --graph --decorate --all"

function gitupload() {
	
	if [ -n "$1" ]; then
		git add .
		git commit -m "$1"
		git push
	else
		echo "Provide a commit message"
	fi
		
}

